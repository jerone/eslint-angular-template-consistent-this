{
  "name": "eslint-plugin-angular-template-consistent-this",
  "version": "0.5.0",
  "description": "ESLint Angular Template consistent this for properties, variables & template references",
  "keywords": [
    "eslint",
    "eslintrule",
    "eslint-rule",
    "eslintplugin",
    "eslint-plugin",
    "angular",
    "angular-template",
    "html"
  ],
  "homepage": "https://github.com/jerone/eslint-plugin-angular-template-consistent-this",
  "repository": {
    "type": "git",
    "url": "https://github.com/jerone/eslint-plugin-angular-template-consistent-this.git"
  },
  "license": "MIT",
  "author": "Jeroen van Warmerdam <jeronevw@hotmail.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "test": "./tests"
  },
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rimraf dist .nyc_output coverage",
    "lint": "lockfile-lint && eslint -f checklist .",
    "lint:fix": "prettier --write \"**/*\" & eslint -f checklist --fix .",
    "prepublishOnly": "npm run build",
    "security:eslint": "eslint -f @microsoft/eslint-formatter-sarif -o eslint-results.sarif .",
    "test": "npm run clean && nyc cross-env TS_NODE_PROJECT=./tsconfig.test.json mocha",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@angular-eslint/template-parser": "14.0.2",
    "@angular-eslint/utils": "14.0.2",
    "@angular/compiler": "14.1.3",
    "@cspell/eslint-plugin": "^6.8.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@types/eslint": "8.4.6",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.13",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "@typescript-eslint/scope-manager": "5.33.1",
    "@typescript-eslint/utils": "5.33.1",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-checklist": "^0.1.0",
    "eslint-plugin-eslint-plugin": "5.0.6",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-markdownlint": "^0.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "lockfile-lint": "^4.8.0",
    "mocha": "10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "@angular-eslint/template-parser": "^14.0.0",
    "eslint": "^7.0.0 || ^8.0.0"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
