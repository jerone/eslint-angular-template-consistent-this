    "@types/mocha": "9.1.1",
    "@types/node": "18.6.4",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "@typescript-eslint/scope-manager": "5.32.0",
